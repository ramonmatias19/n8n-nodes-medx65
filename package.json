{
	"name": "n8n-nodes-medx65",
	"version": "0.1.3",
	"description": "n8n nodes to interact with MedX65 medical system API",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://github.com/ramonmatias19/n8n-nodes-medx65",
	"author": {
		"name": "Ramon Matias",
		"email": "contato@lumiaria.com.br"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ramonmatias19/n8n-nodes-medx65.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/MedX65Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/MedX65/MedX65.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^18.16.16",
		"@types/request": "^2.48.12",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"typescript": "^4.8.4"
	},
	"peerDependencies": {
		"n8n-workflow": "^1.0.0"
	}
}
